<h1>Trained Data on Gender, Height, and Weight</h1>
<table>
<thead>
  <tr>
    <th>Sex</th>
    <th>Height</th>
    <th>Weight</th>
    <th colspan="3"></th>
  </tr>
</thead>
<tbody>
  <% @people.each do |person| %>
    <tr>
      <td><%= person.sex %></td>
      <td><%= person.height %></td>
      <td><%= person.weight %></td>
      <td><%= link_to 'Show', person %></td>
      <td><%= link_to 'Edit', edit_person_path(person) %></td>
      <td><%= link_to 'Destroy', person, method: :delete, data: { confirm: 'Are you sure?' } %></td>
    </tr>
  <% end %>
</tbody>
</table>






